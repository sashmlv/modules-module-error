{
   "name": "module-error",
   "version": "0.0.3",
   "description": "module-error",
   "author": "sashmlv",
   "license": "ISC",
   "main": "./dist/index",
   "scripts": {
      "build:dev": "NODE_ENV=development webpack --config ./build/webpack.config",
      "build:prod": "NODE_ENV=production webpack --config ./build/webpack.config",
      "install": "NODE_ENV=production webpack --config ./build/webpack.config",
      "prepare": "NODE_ENV=production webpack --config ./build/webpack.config",
      "test": "npm run build:prod && ava"
   },
   "repository": {},
   "keywords": [
      "error"
   ],
   "dependencies": {},
   "devDependencies": {
      "ava": "^2.4.0",
      "clean-webpack-plugin": "^3.0.0",
      "jsdom": "^16.2.2",
      "webpack": "^4.43.0",
      "webpack-cli": "^3.3.11"
   },
   "ava": {
      "files": [
         "!*.js",
         "./*.test.js"
      ],
      "verbose": true,
      "timeout": "5s",
      "failFast": true,
      "serial": true
   }
}
